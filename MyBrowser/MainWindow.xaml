        <Button x:Name="Fwdbutton" Content="FWD" HorizontalAlignment="Left" Margin="658,10,0,0" VerticalAlignment="Top" Width="49" Height="19" Click="Fwdbutton_Click"/>
        <WebBrowser x:Name="Browser" Margin="4.6,34,4.6,5.2"/>

    </Grid>
</Window>
